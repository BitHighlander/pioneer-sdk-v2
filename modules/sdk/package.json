{
  "name": "@pioneer-sdk/sdk-v2",
  "version": "0.0.14",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "npm": "npm i",
    "npm-update": "npm update",
    "test": "npm run build && node __tests__/test-module.js",
    "test-ts": "rm -R lib && npm run build && ts-node __tests__/test-module-ts.ts",
    "lint": "prettier --write '**/**/*.ts'",
    "start": "nodemon --watch 'src/**/*.ts' --exec ts-node __tests__node",
    "build": "tsc -p .",
    "build:watch": "npm run build && onchange 'src/**/*.ts' -- npm run build",
    "prepublish": "npm run build",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "test-jest": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map lib node_modules"
  },
  "dependencies": {
    "@keepkey/keepkey-sdk": "^0.2.52",
    "@keplr-wallet/types": "^0.12.31",
    "@pioneer-platform/loggerdog": "^8.1.27",
    "@pioneer-platform/pioneer-caip": "^8.1.81",
    "@pioneer-platform/pioneer-client": "^9.0.19",
    "@pioneer-platform/pioneer-coins": "^8.1.72",
    "@pioneer-platform/pioneer-events": "^8.1.52",
    "@pioneer-platform/pioneer-invoke": "^8.1.43",
    "@coinmasters/api": "latest",
    "@coinmasters/core": "latest",
    "@coinmasters/helpers": "latest",
    "@coinmasters/tokens": "latest",
    "@coinmasters/toolbox-cosmos": "latest",
    "@coinmasters/toolbox-evm": "latest",
    "@coinmasters/toolbox-utxo": "latest",
    "@coinmasters/types": "latest",
    "@coinmasters/wallet-metamask": "latest",
    "@coinmasters/wallet-evm-extensions": "latest",
    "@coinmasters/wallet-keepkey": "latest",
    "@coinmasters/wallet-xdefi": "latest",
    "@coinmasters/wallet-okx": "latest",
    "@coinmasters/wallet-keplr": "latest",
    "@coinmasters/wallet-keystore": "latest",
    "@coinmasters/wallet-ledger": "latest",
    "@coinmasters/wallet-trezor": "latest",
    "@coinmasters/wallet-wc": "latest",    
    "onchange": "^7.1.0",
    "typescript": "^5.2.2"
  }
}
